<h1>Articles</h1>

<div class="conatiner">
  <div class="row">
    <% @articles.each do |article| %>
      <div class="col-xs-12 col-md-3 col-md-offset-1 ">
        <h4><%= link_to article.title, article_path(article) %></h4>
        <h5>Subtitle: <%= article.subtitle %></h5>
        <h5>Published: <%= article.publishing_date %></h5>
      </div>
    <% end %>
  </div>
</div>
